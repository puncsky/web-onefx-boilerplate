{
  "name": "web-onefx-boilerplate",
  "version": "1.1.0",
  "description": "field_description",
  "main": "server.js",
  "scripts": {
    "test": "npm run flow && npm run cover",
    "check-coverage": "nyc check-coverage --lines 65 --functions 60 --Statements 65",
    "ava": "NODE_ENV=test ava",
    "watch": "gulp watch",
    "build": "gulp build",
    "build-production": "NODE_ENV=production gulp build",
    "server": "node server",
    "lint": "eslint . --fix",
    "postinstall": "npm run flow-install && npm run bp && npm run doc",
    "cover": "NODE_ENV=test npm run lint && nyc npm run ava",
    "flow": "flow stop && flow",
    "flow-install": "flow-typed install && flow-typed create-stub graphql-iso-date",
    "size": "ls -lh dist/",
    "bp": "npm run build-production && npm run size",
    "kill": "lsof -n -i4TCP:4000 | grep LISTEN | awk '{ print $2 }' | xargs kill",
    "doc": "graphdoc --config ./package.json --force"
  },
  "graphdoc": {
    "ga": "UA-XXXXXXXX",
    "graphiql": "/api-gateway/",
    "logo": "<a href=\"/\" target=\"_blank\" style=\"display:block;padding:1rem 3rem\"><img src=\"/favicon.png\" /></a>",
    "schemaFile": "./src/api-gateway/api-gateway.graphql",
    "output": "./dist/doc/api-gateway/",
    "baseUrl": "./"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/__tests__/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "cobertura"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "temp-directory": "./coverage/.nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/puncsky/template_website.git"
  },
  "keywords": [],
  "author": "field_author",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/puncsky/template_website/issues"
  },
  "homepage": "https://github.com/puncsky/template_website#readme",
  "devDependencies": {
    "ava": "1.1.0",
    "nyc": "13.1.0",
    "flow-bin": "0.93.0",
    "flow-typed": "2.5.1",
    "eslint": "5.12.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-react": "7.12.4",
    "@2fd/graphdoc": "2.4.0",
    "sw-precache-webpack-plugin": "0.11.5",
